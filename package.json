{
  "name": "Gym-Management-Backend",
  "version": "1.0.0",
  "description": "Gym management Service Backend using TypeScript",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prettier:fix": "prettier --write .",
    "lint-prettier": "yarn lint:check && yarn prettier:check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "postinstall": "prisma generate",
    "migrate": "npx prisma migrate dev",
    "format": "npx prisma format",
    "studio": "npx prisma studio"
  },
  "repository": "https://github.com/jahidulislamjahid/Gym-Management-Backend.git",
  "author": "jahid <info.jahidulislamjahid@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^1.7.3",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.4.2",
    "zod": "^3.22.4"
  }
}
